<Window x:Class="MemSearch.Assembler"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MemSearch"
        mc:Ignorable="d"
        Title="Assembler" Height="87" Width="549" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Background="#FF979797">
    <Grid Height="108" VerticalAlignment="Top">
        <TextBox x:Name="AssemblerBox" HorizontalAlignment="Left" Margin="10,12,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="331" RenderTransformOrigin="0.5,0.5" Height="26" KeyDown="TextBox_KeyDown">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.29"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <Button Content="Assemble" HorizontalAlignment="Left" Margin="344,10,0,0" VerticalAlignment="Top" Height="30" Width="183" Click="Button_Click"/>
    </Grid>
</Window>
